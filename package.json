{
  "name": "material-cms",
  "version": "0.9.7",
  "description": "Material CMS - materialized CMS with reusable widgets based on Apostrophe CMS",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev:start": "npx pm2 start app.js deploy/dev.config.js",
    "dev:reload": "npx pm2 reload material-cms-dev",
    "dev:restart": "npx pm2 restart material-cms-dev",
    "dev:stop": "npx pm2 stop material-cms-dev",
    "dev:docker:up": "docker compose -f ./docker-compose.dev.yml up -d --wait",
    "dev:docker:down": "docker compose -f ./docker-compose.dev.yml down",
    "prod:start": "npx pm2 start app.js deploy/prod.config.js",
    "prod:reload": "npx pm2 reload material-cms",
    "prod:restart": "npx pm2 restart material-cms",
    "prod:stop": "npx pm2 stop material-cms"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Felix Lberg",
  "license": "MIT",
  "dependencies": {
    "apostrophe": "^2.227.12",
    "apostrophe-caches-redis": "^2.1.6",
    "apostrophe-favicons": "^2.0.7",
    "apostrophe-forms": "^1.12.0",
    "apostrophe-open-graph": "^1.1.6",
    "apostrophe-optimizer": "^2.0.2",
    "apostrophe-palette": "^2.0.30",
    "apostrophe-pieces-orderings-bundle": "^2.1.3",
    "apostrophe-rich-text-permalinks": "^2.0.5",
    "apostrophe-seo": "^1.7.0",
    "apostrophe-site-map": "^2.10.0",
    "html-minifier": "^4.0.0",
    "mozjpeg": "^7.1.1",
    "pm2": "^6.0.14",
    "redis": "^3.1.2"
  },
  "devDependencies": {
    "apostrophe-profiler": "^2.1.1"
  }
}
